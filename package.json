{
  "name": "test",
  "description": "",
  "homepage": "https://phovea.caleydo.org",
  "version": "1.0.0-SNAPSHOT",
  "author": {
    "name": "oltionchampari",
    "email": "oltion.champari@datavisyn.io",
    "url": ""
  },
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/phovea/test/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/phovea/test.git"
  },
  "devDpenedencies": {
    "shx": "^0.3.2",
    "gh-pages": "^3.0.0"
  },
  "main": "build/test.js",
  "files": [
    "src",
    "index.js",
    "phovea.js",
    "phovea_registry.js",
    "build"
  ],
  "engines": {
    "npm": ">= 6.12",
    "node": ">= 12.13"
  },
  "scripts": {
    "compile": "tsc",
    "lint": "tslint -c tslint.json -p . 'src/**/*.ts?(x)' 'tests/**/*.ts?(x)'",
    "publish:docs": "gh-pages -d build/docs --message '[skip ci] generate docs'",
    "clean:docs": "rimraf build/docs && mkdir -m777 -p build/docs && shx --verbose cp -R docs/README.md build/docs/;",
    "docs": "npm run docs:web && npm run docs:python",
    "prebuild": "echo hybrid",
    "pretest": "echo hybrid",
    "test": "npm run test:web && npm run test:python",
    "posttest": "echo hybrid",
    "test:watch": "jest --watch",
    "build:dev": "webpack",
    "build": "npm run build:web && npm run build:python",
    "start": "webpack-dev-server --inline",
    "start:hot": "webpack-dev-server --inline --hot",
    "watch": "webpack --watch",
    "release:major": "npm version major && npm publish && git push --follow-tags",
    "release:minor": "npm version minor && npm publish && git push --follow-tags",
    "release:patch": "npm version patch && npm publish && git push --follow-tags",
    "predist": "echo hybrid",
    "dist": "npm run dist:web && npm run dist:python",
    "check": "flake8 --exclude=.git,venv,deploy,docs,__pycache__,node_modules",
    "docs:web": "typedoc --options typedoc.json --name test ./src/index.ts",
    "docs:python": "sphinx-apidoc -o docs -f ./test && sphinx-build ./docs build/docs/backend",
    "pretest:web": "npm run compile",
    "pretest:python": "npm run check",
    "posttest:web": "npm run lint",
    "test:web": "test ! $(find tests -name \"*.ts\") || jest",
    "test:python": "test ! $(find tests -name \"*.py\") || python setup.py test",
    "prebuild:python": "node -e \"process.exit(process.env.PHOVEA_SKIP_TESTS === undefined?1:0)\" || npm run test:python",
    "prebuild:web": "node -e \"process.exit(process.env.PHOVEA_SKIP_TESTS === undefined?1:0)\" || npm run test:web",
    "build:python": "rm -rf build/source && find . -name '*.pyc' -delete && node buildPython.js && cp -r ./test build/source/",
    "build:web": "webpack --env prod",
    "predist:web": "npm run build:web && npm run docs:web",
    "predist:python": "npm run build:python && npm run docs:python",
    "dist:web": "mkdir dist && cd build && tar cvzf ../dist/test.tar.gz *",
    "dist:python": "python setup.py sdist bdist_wheel"
  },
  "devDependencies": {
    "@types/jest": "24.9.1",
    "awesome-typescript-loader": "3.1.2",
    "cache-loader": "1.2.0",
    "css-loader": "0.28.0",
    "extract-text-webpack-plugin": "2.1.0",
    "extract-loader": "0.1.0",
    "identity-obj-proxy": "^3.0.0",
    "ifdef-loader": "2.0.0",
    "file-loader": "0.11.1",
    "fork-ts-checker-webpack-plugin": "0.4.4",
    "html-loader": "0.4.5",
    "imports-loader": "0.7.1",
    "jest": "24.9.0",
    "jest-raw-loader": "1.0.1",
    "json-loader": "0.5.4",
    "node-sass": "^4.13.1",
    "null-loader": "0.1.1",
    "raw-loader": "0.5.1",
    "sass-loader": "6.0.7",
    "style-loader": "0.16.1",
    "thread-loader": "1.1.2",
    "ts-loader": "4.0.1",
    "tslib": "~1.11.0",
    "tslint": "5.9.1",
    "ts-jest": "24.3.0",
    "typedoc": "~0.16.10",
    "typescript": "~3.8.2",
    "url-loader": "0.5.8",
    "webpack": "2.3.3",
    "webpack-dev-server": "2.4.2"
  },
  "dependencies": {
    "phovea_core": "github:phovea/phovea_core#develop"
  }
}
